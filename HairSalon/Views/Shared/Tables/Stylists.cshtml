@using HairSalon.Models;
@model List<HairSalon.Models.Stylist>;
<hr />
<table>
  <thead>
    <tr>
      <th style='width: 12%'>Stylist ID</th>
      <th>Stylist Name</th>
      <th style='width: 20%'></th>
    </tr>
  </thead>
  <tbody>
@foreach (Stylist s in Model)
{
  <tr>
    <td>@s.StylistId</td>
    <td>@s.Name</td>
    <td>@Html.ActionLink("Details", "Details", "Stylists", new { id = s.StylistId }) | 
        @Html.ActionLink("Edit", "Edit", "Stylists", new { id = s.StylistId }) | 
        @Html.ActionLink("Delete", "Delete", "Stylists", new { id = s.StylistId})
    </td>
  </tr>
}
  </tbody>
</table>
<hr />
